<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
	<key>Label</key>
	<string>net.nexp.netserver</string>
	<key>EnvironmentVariables</key>
	<dict>
		<key>NETSERVER_HOME</key>
		<string>/usr/local/netserver</string>
	</dict>
	<key>ProgramArguments</key>
	<array>
		<string>/usr/local/netserver/bin/netserver</string>
	</array>
	<key>KeepAlive</key>
	<true/>
	<key>RunAtLoad</key>
	<true/>
</dict>
</plist>
